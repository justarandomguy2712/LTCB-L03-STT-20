#include <stdio.h>
int main (){
	int n;
	scanf("%d",&n);
	int a[n];
    int *ptr=a;
    for(int i=0;i<n;i++){
    	scanf("%d",ptr+i);
	}
	int temp;
	for(int i=0;i<n-1;i++){
		for(int j=i+1;j<n;j++){
			if(*(ptr+i)>*(ptr+j)){
				temp=*(ptr+i);
				*(ptr+i)=*(ptr+j);
				*(ptr+j)=temp;
			}
		}
	}
	for(int i=0;i<n;i++){
		printf(" %d",*(ptr+i));
	}
	
}
