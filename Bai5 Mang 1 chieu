#include <stdio.h>
#include <limits.h>
int main(){
    int n;
    scanf("%d", &n);
    int a[n];
    for(int i = 0; i < n; i++){
        scanf("%d", &a[i]);
    }
   int x;
   scanf("%d",&x);
   int check=0;
   for(int i=0;i<n;i++){
   	if(a[i]==x){
   		check++;
	   }
   }
   printf("So lan xuat hien cua phan tu x trong mang la: %d",check);
}
