#include <stdio.h>
#include <limits.h>
int main (){
	int n;
	scanf("%d",&n);
	int a[n];
	int *ptr;
	ptr=&a[0];
	for(int i=0;i<n;i++){
		scanf("%d",ptr+i);
	}
	int max=*ptr;
	int min=*ptr;
	for(int i=0;i<n;i++ ){
		if(*(ptr+i)>max){
			max=*(ptr+i);
		}
		if(*(ptr+i)<min){
			min=*(ptr+i);
		}
	}
	printf("\n%d",max);
	printf("\n%d",min);
}
