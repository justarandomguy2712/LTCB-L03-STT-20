#include <stdio.h>
#include <stdbool.h>
int hoanhao(int n){
	int sum=0;
	for(int i=1;i<=n;i++){
		if(n%i==0){
			sum+=i;
		}
	}
	if (sum==2*n){
		return true;
	} else {
		return false;
	}
}
int main (){
	int a;
	scanf("%d",&a);
	bool check=false;
	for(int i=1;i<=a;i++){
		if(hoanhao(a)){
			printf("YES");
		     check=true;
		     break;
		}
		}
	if(check==false){
		printf("NO");
	}
	 }

	
